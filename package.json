{
  "name": "foodcycle-bot",
  "version": "1.0.0",
  "description": "A system that connects food donors with recipients to reduce food waste",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "cd frontend && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-client": "cd frontend && npm install",
    "install-all": "npm install && npm run install-client",
    "build": "cd frontend && npm run build"
  },
  "keywords": [
    "food",
    "donation",
    "waste",
    "chatbot",
    "agents"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "socket.io": "^4.7.2",
    "sqlite3": "^5.1.6",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
} 